#include <stdio.h>

int main(void)
{
    int a, b, c, soma = 0, x;

    printf("Digite tres numeros inteiros, sendo que o primeiro seja maior que 1:\n");
    scanf("%d", &a);
    scanf("%d", &b);
    scanf("%d", &c);

    for (x=b; x<=c; x++)
    {
        if (x%a == 0) soma = soma + x;
    }
    printf("O resultado da soma dos inteiros entre %d e %d foi = %d", b, c, soma);

    return(0);
}
